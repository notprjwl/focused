{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/api/user/signup",
      "dest": "https://focused-server.vercel.app/api/user/signup",
      "methods": ["POST"]
    }
  ]
}
